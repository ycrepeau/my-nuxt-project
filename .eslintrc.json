{
    "env": {
        "node": true,
        "commonjs": true,
        "es2021": true
    },
    "extends": [
        "airbnb-base", "prettier"
    ],

    "plugins": ["prettier"],
    "rules": {
        "prettier/prettier": "error"        
    },
    "parser": "vue-eslint-parser",
    "parserOptions": {
        "ecmaVersion": 12
    }
}
